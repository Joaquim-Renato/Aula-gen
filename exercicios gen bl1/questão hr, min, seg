programa
{
	
	funcao inicio()
	{
	
	real horas, minutos, segundos, res

	
	escreva("entre com os segundos:") 
	leia(segundos) 
	
escreva("entre com os minutos:")
	leia(minutos)
	
	escreva("entre com as horas:")
	leia(horas)
	
	res= segundos * minutos * horas

	escreva("multiplicação:", res) 
	}
}
